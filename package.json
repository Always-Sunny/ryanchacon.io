{
  "name": "ryanchacon",
  "version": "1.0.0",
  "description": "personal website",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node ./server/server.js --ignore client/",
    "dev": "webpack -wd --config ./webpack.config.dev.js",
    "ss": "babel-node ./server/server.js",
    "lint": "eslint client/src",
    "lint:watch": "esw client/src --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node ./server/buildHtml.js",
    "prebuild": "npm-run-all clean-dist copy ",
    "build": "babel-node ./server/build.js",
    "postbuild": "babel-node ./server/distServer.js",
    "copy": "cp -r ./public/assets ./dist"
  },
  "author": "RC",
  "repository": {
    "type": "git",
    "url": "git://github.com/Always-Sunny/ryanchacon.io.git"
  },
  "license": "ISC",
