    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.1.2",
    "compression-webpack-plugin": "^1.1.3",
    "css-hot-loader": "^1.3.5",
